{
  "identifier": "com.roojay.bobplugin-google-translate",
  "category": "translate",
  "version": "0.2.2",
  "name": "Google 翻译",
  "summary": "Bob google 翻译插件",
  "author": "roojay <roojay520@gmail.com>",
  "homepage": "https://github.com/roojay520/bobplugin-google-translate",
  "icon": "114",
  "minBobVersion": "0.5.0",
  "options": [
    {
      "identifier": "cache",
      "type": "menu",
      "title": "缓存",
      "defaultValue": "enable",
      "menuValues": [
        {
          "title": "开启",
          "value": "enable"
        },
        {
          "title": "关闭",
          "value": "disable"
        }
      ]
    },
    {
      "identifier": "tld",
      "type": "menu",
      "title": "接口域名",
      "defaultValue": "cn",
      "menuValues": [
        {
          "title": "google.cn(国内)",
          "value": "cn"
        },
        {
          "title": "google.com(国际)",
          "value": "com"
        }
      ]
    }
  ]
}
